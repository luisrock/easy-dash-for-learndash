//Gets the list of enrolled courses for a group.
learndash_group_enrolled_courses( int $group_id,  boolean $bypass_transient = false ) 
//Gets the list of enrolled courses for a group.
learndash_get_group_courses_list( $group_id = 0 )
//Gets the list of groups associated with the course.
learndash_get_course_groups( int $course_id,  boolean $bypass_transient = false ) 
//Gets the list of users ids that belong to a group.
learndash_get_groups_user_ids( int $group_id,  boolean $bypass_transient = false )
//Gets the list of user objects that belong to a group.
learndash_get_groups_users( $group_id, $bypass_transient = false )
//Gets the list of administrator IDs for a group.
learndash_get_groups_administrator_ids( $group_id = 0, $bypass_transient = false )
//Gets the list of group leaders for the given group ID.
learndash_get_groups_administrators( $group_id = 0, $bypass_transient = false )
//Get all Users within all Groups managed by the Group Leader.
learndash_get_groups_administrators_users( $group_leader_id = 0 )
//Get all Courses within all Groups managed by the Group Leader.
learndash_get_groups_administrators_courses( $group_leader_id = 0 )
//Checks whether a group is enrolled in a course or not.
learndash_group_has_course( $group_id = 0, $course_id = 0 )
//Gets the timestamp of when a course is available to the group.
learndash_group_course_access_from( $group_id = 0, $course_id = 0 )
//Checks whether a course can be accessed by the user's group.
learndash_user_group_enrolled_to_course( $user_id = 0, $course_id = 0 )
//Gets timestamp of when the course is available to a user in a group.
learndash_user_group_enrolled_to_course_from( $user_id = 0, $course_id = 0, $bypass_transient = false )
//Gets the list of group IDs administered by the user.
learndash_get_administrators_group_ids( $user_id, $_MENU = false )
//Gets the list of all groups.
learndash_get_groups( $id_only = false, $current_user_id = 0 )
//Gets all quizzes related to Group Courses. * se já tiver sido usada a função que busca cursos do grupo, será usada novamente. (não bom)
learndash_get_group_course_quiz_ids( $group_id = 0 )
//Check and recalculate the the status of the Group Courses for the User.
learndash_get_user_group_progress( $group_id = 0, $user_id = 0, $recalc = false )
//Get User's group status
learndash_get_user_group_status( $group_id = 0, $user_id = 0, $return_slug = false )
//Get the user started group timestamp.
learndash_get_user_group_started_timestamp
//Get the user completed group timestamp.
learndash_get_user_group_completed_timestamp( $group_id = 0, $user_id = 0 )
//Get the user completed group percentage.
learndash_get_user_group_completed_percentage($group_id = 0, $user_id = 0 )
//Return all groups below the parent.
learndash_get_group_children( $group_id = 0 )
//Get all Courses having Group associations.
learndash_get_all_courses_with_groups()

